    "import pandas as pd\n",
    "\n",
    "data = pd.read_csv(\"./data/games_price/約旦_info.csv\")\n",
    "data[\"價格\"] = data[\"價格\"].str.replace(r'[^\\d.]', '', regex=True)\n",
    "data = data.dropna(subset=[\"價格\"])\n",
    "data = data.query(\"價格 != ''\")\n",
    "data[\"價格\"] = data[\"價格\"].astype(float)\n",
    "data"
